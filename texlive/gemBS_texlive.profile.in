selected_scheme scheme-infraonly
TEXDIR @GEMBS_INSTALL_ROOT/texlive
TEXMFCONFIG ~/.texlive/texmf-config
TEXMFHOME ~/texmf
TEXMFLOCAL @GEMBS_INSTALL_ROOT/texlive/texmf-local
TEXMFSYSCONFIG @GEMBS_INSTALL_ROOT/texlive/texmf-config
TEXMFSYSVAR @GEMBS_INSTALL_ROOT/texlive/texmf-var
TEXMFVAR ~/.texlive/texmf-var
binary_x86_64-linux 1
instopt_adjustpath 0
instopt_adjustrepo 1
instopt_letter 0
instopt_portable 0
instopt_write18_restricted 1
tlpdbopt_autobackup 0
tlpdbopt_backupdir tlpkg/backups
tlpdbopt_create_formats 1
tlpdbopt_desktop_integration 0
tlpdbopt_file_assocs 0
tlpdbopt_generate_updmap 0
tlpdbopt_install_docfiles 0
tlpdbopt_install_srcfiles 0
tlpdbopt_post_code 1
tlpdbopt_sys_bin @INSTALL_PREFIX/bin
tlpdbopt_sys_info @INSTALL_PREFIX/share/info
tlpdbopt_sys_man @INSTALL_PREFIX/share/man
tlpdbopt_w32_multi_user 1
